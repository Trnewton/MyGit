#include <stdio.h>
#include <stdlib.h>

#define MAXOP 20
#define NUMBER '0'
#define TOOBIG '9'

int push(int f);
int pop();
void clear();
int getop(char *s, int lim);
int getch();


int main() {
	int c, i;
	c = 0;
	for(i = 1; (c = getch()) > 0; i++){
		printf("c: %d\n",c);
		printf("i: %d\n",i);
		if(i==3)
			break;
	}

}

int getch(){
	return(1);
}
